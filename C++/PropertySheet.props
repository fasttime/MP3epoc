<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <MessageResourceDir>$(ProjectDir)winres</MessageResourceDir>
    <BuildDir>$(SolutionDir)Visual Studio Build\</BuildDir>
    <OutDir>$(BuildDir)$(ProjectName) $(Configuration) Output Files\</OutDir>
    <IntDir>$(BuildDir)$(ProjectName) $(Configuration) Intermediate Files\</IntDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="MessageResourceDir">
      <Value>$(MessageResourceDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="BuildDir">
      <Value>$(BuildDir)</Value>
    </BuildMacro>
    <BuildMacro Include="OutDir">
      <Value>$(OutDir)</Value>
    </BuildMacro>
    <BuildMacro Include="IntDir">
      <Value>$(IntDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>